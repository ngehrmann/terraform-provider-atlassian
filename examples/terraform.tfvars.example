# Copy this file to terraform.tfvars and fill in your actual values

# Atlassian API token (get from https://id.atlassian.com/manage-profile/security/api-tokens)
atlassian_api_token = "your-api-token-here"

# Email associated with your Atlassian account
atlassian_email = "your-email@example.com"

# Your Atlassian organization/site name (e.g., "mycompany" for mycompany.atlassian.net)
atlassian_organization = "your-organization"

# Base URL (optional - defaults to https://api.atlassian.com)
# atlassian_base_url = "https://api.atlassian.com"